{
  "name": "swiss-car-rental-management",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "eslint . --fix --cache",
    "lint:check": "eslint . --cache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:unit": "turbo test:unit",
    "test:e2e": "turbo test:e2e",
    "test:coverage": "turbo test:coverage",
    "clean": "turbo clean && rm -rf node_modules .turbo",
    "clean:all": "pnpm clean && pnpm -r exec rm -rf node_modules",
    "ui:add": "pnpm dlx shadcn-ui@latest add",
    "ui:diff": "pnpm dlx shadcn-ui@latest diff",
    "db:generate": "pnpm --filter @car-rental/database db:generate",
    "db:push": "pnpm --filter @car-rental/database db:push",
    "db:migrate": "pnpm --filter @car-rental/database db:migrate",
    "db:migrate:deploy": "pnpm --filter @car-rental/database db:migrate:deploy",
    "db:reset": "pnpm --filter @car-rental/database db:reset",
    "db:seed": "pnpm --filter @car-rental/database db:seed",
    "db:studio": "pnpm --filter @car-rental/database db:studio",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "commit": "cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@types/node": "^22",
    "eslint": "^9.32.0",
    "eslint-config-turbo": "^2.5.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "turbo": "^2.3.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.39.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.15.0"
  },
  "packageManager": "pnpm@9.15.0"
}
