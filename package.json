{
  "name": "swiss-car-rental-management",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "eslint . --fix --cache",
    "lint:check": "eslint . --cache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "clean": "turbo clean && rm -rf node_modules .turbo",
    "clean:all": "pnpm clean && pnpm -r exec rm -rf node_modules",
    "ui:add": "pnpm dlx shadcn-ui@latest add",
    "ui:diff": "pnpm dlx shadcn-ui@latest diff",
    "db:generate": "pnpm --filter @swiss-car-rental/database db:generate",
    "db:push": "pnpm --filter @swiss-car-rental/database db:push",
    "db:migrate": "pnpm --filter @swiss-car-rental/database db:migrate",
    "db:migrate:deploy": "pnpm --filter @swiss-car-rental/database db:migrate:deploy",
    "db:reset": "pnpm --filter @swiss-car-rental/database db:reset",
    "db:seed": "pnpm --filter @swiss-car-rental/database db:seed",
    "db:studio": "pnpm --filter @swiss-car-rental/database db:studio",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "commit": "cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^9.3.0",
    "@playwright/test": "^1.54.2",
    "@axe-core/playwright": "^4.10.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-turbo": "^2.5.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "msw": "^2.10.4",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "turbo": "^2.3.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.15.0"
  },
  "packageManager": "pnpm@9.15.0"
}
