<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for development dependencies -->
    <suppress>
        <notes>Development dependencies - not used in production</notes>
        <gav regex="true">.*:.*:.*</gav>
        <cpe>cpe:/a:.*:.*:.*</cpe>
        <vulnerabilityName regex="true">.*test.*</vulnerabilityName>
    </suppress>
    
    <!-- Suppress known false positives in Next.js -->
    <suppress>
        <notes>Next.js framework - managed by Vercel</notes>
        <packageUrl regex="true">^pkg:npm/next@.*</packageUrl>
        <cve>CVE-2021-44906</cve>
    </suppress>
    
    <!-- Suppress TypeScript development tools -->
    <suppress>
        <notes>TypeScript is a development dependency</notes>
        <packageUrl regex="true">^pkg:npm/typescript@.*</packageUrl>
    </suppress>
    
    <!-- Suppress ESLint and related tools -->
    <suppress>
        <notes>ESLint is a development dependency</notes>
        <packageUrl regex="true">^pkg:npm/eslint.*@.*</packageUrl>
    </suppress>
</suppressions>